  "scripts": {
    "build": "microbundle -i ./src/gitlogs.js -o ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjanssen/gitlogs.git"
  },
  "main": "./dist/index.js",
  "prettier": {

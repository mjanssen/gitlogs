    "singleQuote": true,
    "printWidth": 100
  },
  "author": {
    "name": "Marnix Janssen",
    "email": "dev.marnix@gmail.com",
    "url": "https://github.com/mjanssen"
  },
  "license": "MIT",
  "dependencies": {
    "dayjs": "^1.7.8",
    "kleur": "^3.0.1",
    "microbundle": "^0.9.0",
    "pkg": "^4.3.5",
    "yargs": "^12.0.5"
  }
}
